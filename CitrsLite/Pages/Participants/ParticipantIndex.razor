@page "/participant"
@using CitrsLite.Data.Models;


<MudContainer>
    <MudCard>
        <MudCardHeader>
            <MudText Typo="Typo.h2">Participant Search</MudText>
        </MudCardHeader>
        <MudCardContent>
            <MudForm>
                <MudTextField Label="Name" @bind-Value="name"/>
            </MudForm>
        </MudCardContent>
    </MudCard>
</MudContainer>

@code {
    private IEnumerable<Participant> participants;
    private string name;

}
